{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Run Dev Server",
			"type": "shell",
			"command": "cmd",
			"args": [
				"/c",
				"npm",
				"run",
				"dev"
			],
			"isBackground": true,
			"problemMatcher": [
				{
					"owner": "custom",
					"pattern": [
						{
							"regexp": ".",
							"file": 1,
							"location": 2,
							"message": 3
						}
					],
					"background": {
						"activeOnStart": true,
						"beginsPattern": "Ready in",
						"endsPattern": "Compiled"
					}
				}
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}
